{
  "manifest_version": 2,
  "name": "ABP Mangacap Helper",
  "description": "Keep ads blocked on Mangacap.com and shows the manga",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": [
        "http://www.mangacap.com/*", "*://mangacap.com/*"
      ],
      "js": ["jquery-2.1.4.min.js", "show-manga.js"],
      "run_at": "document_idle"
    }
  ]
}
